{
  "name": "@twilio-labs/svg-to-react",
  "version": "2.0.0",
  "description": "A utility to convert raw svg files into accessible and extendable React Components.",
  "author": "Twilio Inc",
  "license": "MIT",
  "main": "dist/main.js",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "index.js"
  ],
  "scripts": {
    "clean": "rimraf coverage dist",
    "build": "tsc -b tsconfig.release.json",
    "build:watch": "tsc -b -w tsconfig.release.json",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "convert": "./index.js convert",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@svgr/babel-plugin-add-jsx-attribute": "^4.2.0",
    "@svgr/babel-plugin-remove-jsx-attribute": "^4.2.0",
    "@svgr/cli": "^4.3.2",
    "@svgr/core": "^4.3.2",
    "@svgr/plugin-jsx": "^4.3.2",
    "@svgr/plugin-svgo": "^4.3.1",
    "prettier": "^1.18.2",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.16",
    "@types/loadable__component": "^5.10.0",
    "@types/node": "^12.6.8",
    "@types/prettier": "^1.18.0",
    "@types/promise-timeout": "^1.3.0",
    "commander": "^2.20.0",
    "inquirer": "^6.5.0",
    "jest": "^24.8.0",
    "jest-runner-prettier": "^0.2.7",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3"
  }
}
